{
    "env": {
      "amd": true,
      "es6": true,
      "mocha": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "prettier",
      "mocha"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-console": "off"
    },
    "parser": "typescript-eslint-parser",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "globals": {
      "Buffer": true,
      "console": true,
      "expect": true,
      "global": true,
      "proxyquire": true,
      "sinon": true,
      "process": true
    }
}
